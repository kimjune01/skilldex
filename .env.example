# Session/JWT
JWT_SECRET=your-jwt-secret-here-change-in-production

# API Key Encryption (optional - falls back to JWT_SECRET if not set)
# Recommended to set a separate key in production for security isolation
# API_KEY_ENCRYPTION_KEY=your-api-key-encryption-key

# Database - Local (SQLite)
DATABASE_URL=file:./data/skillomatic.db

# Database - Production (Turso) - only set these for production
# TURSO_DATABASE_URL=libsql://your-db.turso.io
# TURSO_AUTH_TOKEN=your-turso-auth-token

# Nango - Local (self-hosted via docker-compose)
NANGO_HOST=http://localhost:3003
NANGO_SECRET_KEY=your-nango-secret-key
NANGO_PUBLIC_KEY=your-nango-public-key
NANGO_DB_PASSWORD=your-nango-db-password
# Webhook signature verification (defaults to NANGO_SECRET_KEY if not set)
NANGO_WEBHOOK_SECRET=your-nango-webhook-secret

# Nango - Production (Nango Cloud) - get keys from https://app.nango.dev
# NANGO_HOST=https://api.nango.dev
# NANGO_SECRET_KEY=your-nango-cloud-secret-key
# NANGO_PUBLIC_KEY=your-nango-cloud-public-key

# ATS Integration
# Set USE_ZOHO_ATS=true to use Zoho Recruit instead of the mock ATS
USE_ZOHO_ATS=false
MOCK_ATS_URL=http://localhost:3001

# Admin (default admin user)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme

# API URLs (for skills)
SKILLOMATIC_API_URL=http://localhost:3000

# LLM (Groq - free tier available at console.groq.com)
GROQ_API_KEY=your-groq-api-key

# Tavily Web Search (https://tavily.com)
TAVILY_API_KEY=
