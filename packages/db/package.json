{
  "name": "@skillomatic/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema.ts",
    "./client": "./src/client.ts"
  },
  "scripts": {
    "push": "drizzle-kit push",
    "migrate": "tsx src/migrate.ts",
    "migrate:prod": "tsx src/migrate-prod.ts",
    "generate": "drizzle-kit generate",
    "studio": "drizzle-kit studio",
    "seed": "tsx seed/index.ts",
    "seed:prod": "tsx src/seed-prod.ts",
    "setup": "tsx src/migrate.ts && tsx seed/index.ts",
    "setup:prod": "tsx src/migrate-prod.ts && tsx src/seed-prod.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "bcrypt-ts": "^8.0.0",
    "better-sqlite3": "^12.6.2",
    "drizzle-orm": "^0.45.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3"
  }
}
